<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="rtfm"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="RegexTagForMusic">
         <property
               name="appName"
               value="RegexTagForMusic">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.core.resources.IProject"
            class="org.essembeh.rtfm.core.project.internal.RtfmProjectAdapterFactory">
         <adapter
               type="org.essembeh.rtfm.core.project.IRtfmProject">
         </adapter>
      </factory>
   </extension>

</plugin>
