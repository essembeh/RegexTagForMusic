<?xml version="1.0"?>
<rtfm>
	<filehandlers>
		<filehandler id="MP3" pattern="/[^/]+/[^/]+/[0-9][0-9]\..*\.mp3" database="true">
			<tagwriter ref="yajil" />
			<tagprovider ref="ALBUM" />
		</filehandler>
		<filehandler id="COVER" pattern="/[^/]+/[^/]+/cover\.(jpg|jpeg|png)" />
		<filehandler id="PLAYLIST" pattern="/[^/]+/[^/]+/playlist\.m3u" />
		<filehandler id="UNKNOWN" pattern=".*" database="false" />
	</filehandlers>
</rtfm>
