<?xml version="1.0"?>
<rtfm>
	<filehandlers>
		<filehandler id="ALBUM" pattern="/Albums/[^/]+/[^/]+/[0-9][0-9] - .*\.mp3" database="true">
			<tagwriter ref="JAudioTagger-ID3v2.3" />
			<tagprovider ref="Album" />
		</filehandler>
		<filehandler id="SINGLE" pattern="/Singles/([^/]+) - ([^/]+)\.mp3" database="true">
			<tagwriter ref="JAudioTagger-ID3v2.3" />
			<tagprovider ref="Single" />
		</filehandler>
		<filehandler id="COVER" pattern="/Albums/[^/]+/[^/]+/cover\.(jpg|jpeg|png)" database="true">
		</filehandler>
		<filehandler id="UNKNOWN" pattern=".*" database="false">
		</filehandler>
	</filehandlers>
</rtfm>
