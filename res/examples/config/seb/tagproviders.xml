<?xml version="1.0"?>
<rtfm>
	<tagproviders>
		<tagprovider id="Album">
			<artist pattern="/Albums/(.*)/.*/[0-9]{2,3} - .*\.mp3" group="1" />
			<album pattern="/Albums/.*/(.*)/[0-9]{2,3} - .*\.mp3" />
			<year pattern="/Albums/.*/([12][0-9][0-9][0-9]) - .*/[0-9]{2,3} - .*\.mp3" optional="true" />
			<tracknumber pattern="/Albums/.*/.*/([0-9]{2,3}) - .*\.mp3" />
			<trackname pattern="/Albums/.*/.*/[0-9]{2,3} - (.*)\.mp3" />
			<comment value="Tagged with RegexTagForMusic" />
		</tagprovider>
		<tagprovider id="Single">
			<artist value="#Single" />
			<album pattern="/Singles/([^/]+) - ([^/]+)\.mp3" group="1" />
			<trackname pattern="/Singles/([^/]+) - ([^/]+)\.mp3" group="2" />
			<comment value="Tagged with RegexTagForMusic" />
		</tagprovider>
		<tagprovider id="Saga">
			<artist value="#Saga" />
			<album pattern="/Saga/([^/]+)/([^/]+)\.mp3" group="1" />
			<trackname pattern="/Saga/([^/]+)/([^/]+)\.mp3" group="2" />
			<comment value="Tagged with RegexTagForMusic" />
		</tagprovider>
	</tagproviders>
</rtfm>
