<?xml version="1.0"?>
<rtfm>
	<filehandlers>
		<filehandler id="MP3" pattern="/[^/ A-Z]+/[^/ A-Z]+/[0-9][0-9]\.[^/ A-Z]+\.mp3" database="true">
			<tagwriter ref="JAudioTagger-ID3v2.4" />
			<tagprovider ref="ALBUM" />
		</filehandler>
		<filehandler id="COVER" pattern="/[^/ A-Z]+/[^/ A-Z]+/cover\.(jpg|jpeg|png)" />
		<filehandler id="PLAYLIST" pattern="/[^/ A-Z]+/[^/ A-Z]+/playlist\.m3u" />
		<filehandler id="UNKNOWN" pattern=".*" database="false" />
	</filehandlers>
</rtfm>
