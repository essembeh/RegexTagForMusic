{
  "substitutions": {},
  "commands": {
    "eyed3-clean": [
      "eyeD3",
      "--no-color",
      "--remove-all",
      "${path}"
    ],
    "delete": [
      "rm",
      "${path}"
    ]
  },
  "workflows": {
    "mp3": {
      "pattern": ".*\\.mp3",
      "execute": [
        "eyed3-clean"
      ]
    },
    "junk": {
      "pattern": ".*/\\.DS_Store",
      "execute": [
        "delete"
      ]
    }
  }
}
