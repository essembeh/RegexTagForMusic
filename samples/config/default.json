{
	"commands": {
		"eyed3-clean": [
			"/usr/bin/eyeD3",
			"--no-color",
			"--remove-all",
			"${FILE}"
		],
		"eyed3-tag": [
			"/usr/bin/eyeD3",
			"--no-color",
			"--artist=${ARTIST}",
			"--album=${ALBUM}",
			"--year=${YEAR}",
			"--track=${TRACK}",
			"--title=${TITLE}",
			"${FILE}"
		]
	},
	"types": {
		"mp3": {
			"pattern": "(?<FILE>.*/(?<ARTIST>[^/]+)/(?:(?<YEAR>[12]\\d{3}) - )?(?<ALBUM>[^/]+)/(?<TRACK>\\d{2,3}) - (?<TITLE>[^/]+)\\.mp3)",
			"variables": { "YEAR": "" },
			"workflow": [ "eyed3-clean", "eyed3-tag" ]
		}
	}
}
