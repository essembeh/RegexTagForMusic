{
  "filters": {
    "mp3": {
      "pattern": "(.*/)?(?P<ARTIST>[^/]+)/((?P<YEAR>[12][0-9]{3}) - )?(?P<ALBUM>[^/]+)/(?P<TRACK>[0-9]{2,3}) - (?P<TITLE>[^/]+)\\.mp3",
      "properties": {
        "eyed3": true,
        "eyed3.artist": {
          "group": "ARTIST"
        },
        "eyed3.album": {
          "group": "ALBUM"
        },
        "eyed3.year": {
          "group": "YEAR"
        },
        "eyed3.track": {
          "group": "TRACK"
        },
        "eyed3.title": {
          "group": "TITLE"
        },
        "eyed3.comment": "Tagged with rtfm"
      }
    },
    "cover": {
      "pattern": "(.*/)?[^/]+/[^/]+/cover.(jpg|png)"
    }
  }
}