{
  "commands": {
    "eyed3-clean": [
      "eyeD3",
      "--no-color",
      "--remove-all",
      "${path}"
    ],
    "eyed3-tag": [
      "eyeD3",
      "--no-color",
      "--artist=${ARTIST}",
      "--album=${ALBUM}",
      "--year=${YEAR}",
      "--track=${TRACK}",
      "--title=${TITLE}",
      "${path}"
    ]
  },
  "workflows": {
    "cover": {
      "pattern": ".*/cover.jpg"
    },
    "mp3": {
      "pattern": ".*/(?<ARTIST>[^/]+)/(?:(?<YEAR>[12]\\d{3}) - )?(?<ALBUM>[^/]+)/(?<TRACK>\\d{2,3}) - (?<TITLE>[^/]+)\\.mp3",
      "variables": {
        "YEAR": ""
      },
      "execute": [
        "eyed3-clean",
        "eyed3-tag"
      ]
    }
  }
}
