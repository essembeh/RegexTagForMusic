{
  "commands": {
    "eyed3-clean": [
      "eyeD3",
      "--no-color",
      "--remove-all",
      "${path}"
    ],
    "eyed3-album-with-year": [
      "eyeD3",
      "--no-color",
      "--artist=${ARTIST}",
      "--album=${ALBUM}",
      "--year=${YEAR}",
      "--track=${TRACK}",
      "--title=${TITLE}",
      "--comment=::Tagged with rtfm",
      "${path}"
    ],
    "eyed3-album-without-year": [
      "eyeD3",
      "--no-color",
      "--artist=${ARTIST}",
      "--album=${ALBUM}",
      "--track=${TRACK}",
      "--title=${TITLE}",
      "--comment=::Tagged with rtfm",
      "${path}"
    ],
    "eyed3-non-album": [
      "eyeD3",
      "--no-color",
      "--artist=${ARTIST}",
      "--album=${ALBUM}",
      "--title=${TITLE}",
      "--comment=::Tagged with rtfm",
      "${path}"
    ],
    "eyed3-utf8": [
      "eyeD3",
      "--no-color",
      "--set-encoding=utf8",
      "--force-update",
      "${path}"
    ],
    "delete": [
      "rm",
      "${path}"
    ]
  },
  "workflows": {
    "album-with-year": {
      "pattern": ".*/Albums/(?<ARTIST>[^/]+)/(?<ALBUM>[^/]+) \\((?<YEAR>[12]\\d{3})\\)/(?<TRACK>\\d{2,3}) (?<TITLE>[^/]+)\\.mp3",
      "execute": [
        "eyed3-clean",
        "eyed3-album-with-year",
        "eyed3-utf8"
      ]
    },
    "album-without-year": {
      "pattern": ".*/Albums/(?<ARTIST>[^/]+)/(?<ALBUM>[^/]+)/(?<TRACK>\\d{2,3}) (?<TITLE>[^/]+)\\.mp3",
      "execute": [
        "eyed3-clean",
        "eyed3-album-without-year",
        "eyed3-utf8"
      ]
    },
    "cover": {
      "pattern": ".*/Albums/[^/]+/[^/]+/cover.jpg"
    },
    "single": {
      "pattern": ".*/Singles/(?<ARTIST>[^/]+) - (?<TITLE>[^/]+)\\.mp3",
      "variables": {
        "ALBUM": "#Single"
      },
      "execute": [
        "eyed3-clean",
        "eyed3-non-album",
        "eyed3-utf8"
      ]
    },
    "saga": {
      "pattern": ".*/Saga/(?<ARTIST>[^/]+)/(?<TITLE>[^/]+)\\.mp3",
      "variables": {
        "ALBUM": "#Saga"
      },
      "execute": [
        "eyed3-clean",
        "eyed3-non-album",
        "eyed3-utf8"
      ]
    },
    "junk": {
      "pattern": ".*/\\.DS_Store",
      "execute": [
        "delete"
      ]
    }
  }
}
